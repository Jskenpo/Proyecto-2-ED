<form method = "post" action = "/movie/add"> 
    <label> Movie Name</label><br>
    <input type = "text" name = "movie_title">
    <br>
    <label>Year</label> <br>
    <input type = "text" name = "movie_year">
    <br>   
    <input type = "submit" value = "Submit">
</form>


<h1>Actors</h1>
<ul>
    <% movies.forEach(function(movie){ %>
        <li><%=movie.title  %></li>
    <% }) %>
</ul>